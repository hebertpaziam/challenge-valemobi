{
    "name": "valemobi-api",
    "author": "Hebert Paziam",
    "version": "1.0.0",
    "private": false,
    "scripts": {
        "start": "tsc && npm run build-css && concurrently \"npm run watch-css\" \"tsc -w\" \"lite-server\" ",
        "bower": "bower install",
        "typigns": "typings install",
        "preinstall": "npm install -g typings && npm install -g typescript && npm install -g bower",
        "postinstall": "npm run typigns && npm run bower && npm start",
        "lint": "tslint ./app/**/*.ts -t verbose",
        "lite": "lite-server",
        "test": "tsc && concurrently \"tsc -w\" \"karma start karma.conf.js\"",
        "test-once": "tsc && karma start karma.conf.js --single-run",
        "tsc": "tsc",
        "tsc:w": "tsc -w",
        "typings": "typings",
        "build-css": "node-sass assets/scss -o assets/css",
        "watch-css": "node-sass -w assets/scss -o assets/css",
        "webdriver:update": "webdriver-manager update"
    },
    "keywords": [],
    "license": "MIT",
    "dependencies": {
        "@angular/common": "^2.0.0",
        "@angular/compiler": "^2.0.0",
        "@angular/core": "^2.0.0",
        "@angular/forms": "^2.0.0",
        "@angular/http": "^2.0.0",
        "@angular/platform-browser": "^2.0.0",
        "@angular/platform-browser-dynamic": "^2.0.0",
        "@angular/router": "^3.0.0",
        "@angular/upgrade": "^2.0.0",
        "core-js": "^2.4.1",
        "font-awesome": "^4.6.3",
        "reflect-metadata": "^0.1.3",
        "rxjs": "^5.0.0-beta.11",
        "systemjs": "0.19.27",
        "zone.js": "^0.7.2"
    },
    "devDependencies": {
        "canonical-path": "0.0.2",
        "concurrently": "^2.2.0",
        "http-server": "^0.9.0",
        "lite-server": "^2.2.2",
        "node-sass": "^3.13.1",
        "tslint": "^3.15.1",
        "typescript": "^1.8.10",
        "typings": "^1.5.0"
    },
    "repository": {
        "homepage": "https://github.com/hebertpazian/valemobi-api"
    }
}